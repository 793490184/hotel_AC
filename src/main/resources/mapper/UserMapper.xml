<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.InitLee.hotel_CA.mapper.UserMapper">

    <insert id="addUser" parameterType="com.InitLee.hotel_CA.entity.User">
        insert into user(
        name,
        password
        ) value (
        #{name},
        #{password}
        )
    </insert>


</mapper>